					<span class="page-name">Tags</span>
				</button>
			</div>

			// <aui:nav>
			<div class="collapse navbar-collapse">
				<ul aria-label="Tags" class="lfr-nav nav navbar-nav" role="menubar">

					// <aui:nav-item>
					<li class="active" role="presentation">
						<a class="" href="http://localhost:8080/group/guest/~/control_panel/manage/-/tags_admin/tags" role="menuitem" title="Tags">
							<span class="nav-item-label">Tags</span>
						</a>
					</li>
					// </aui:nav-item>

				</ul>
			</div>
			// </aui:nav>

			{call .aui_nav_bar_search /}
		</div>
	</div>
{/template}

