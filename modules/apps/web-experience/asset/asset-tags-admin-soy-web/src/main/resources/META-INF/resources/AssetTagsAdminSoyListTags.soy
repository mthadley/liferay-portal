{namespace AssetTagsAdminSoyListTags}

/**
 * Prints the Tags view page
 *
 * @param breadcrumbs
 * @param id
 * @param themeDisplay
 */
{template .render}
	<div id="{$id}">
		{call .aui_nav_bar /}

		{call LexiconBreadcrumbs.render}
			{param items: $breadcrumbs /}
			{param ref: 'breadcrumbs' /}
		{/call}

		<div class="management-bar-container" data-qa-id="managementBar">
			/*
			{call Lexicon_ManagementBar.render}
				{param buttons: $managementBar.buttons /}
				{param filters: $managementBar.filters /}
				{param showCheckbox: $managementBar.showCheckbox /}
				{param spritesheet: $spritesheet /}
			{/call}
			*/
		</div>

		/*
		{call SearchContainer.render data="all"}
			{param contentTemplate: 'AssetTagsAdminSoyListTags' /}
			{param displayStyle: $displayStyle /}
			{param items: $tags /}
			{param key: $id + 'SearchContainer' /}
		{/call}
		*/
		{call LexiconButton.render data="all"}
			{param elementClasses: 'btn-action btn-bottom-right' /}
			{param icon: 'plus' /}
			{param level: 'primary' /}
			{param spritemap: $themeDisplay.pathThemeImages +'/lexicon/icons.svg' /}
		{/call}
	</div>
{/template}

/**
 *
 */
{template .aui_nav_bar}
